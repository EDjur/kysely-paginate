{
  "name": "@edjur/kysely-paginate",
  "version": "1.0.0",
  "description": "Pagination helpers for use with kysely (FORK)",
  "repository": {
    "type": "git",
    "url": "git://github.com/EDjur/kysely-paginate.git"
  },
  "author": "Elias Djurfeldt <eliasdjurfeldt@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16.14.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "release": "pnpm run build && pnpm changeset publish",
    "publish": "npm publish --access public",
    "bench": "vitest bench",
    "lint": "eslint .",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "better-sqlite3": "^12.6.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "kysely": "^0.28.10",
    "mysql2": "^3.16.2",
    "pg": "^8.17.2",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "kysely": "*"
  },
  "files": [
    "./dist"
  ]
}
